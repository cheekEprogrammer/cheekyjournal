<%- include("partials/header"); %>

<% if (posts.length === 0) { %>
  <h1>Home</h1>
  <p>
    <%= startingContent %>
  </p>
<% } else { %>
  <% posts.forEach(function(post){ %>
    <h1><%= post.title %></h1>
    <p><%= post.content %></p>
  <% }); %>
<% } %>






<%- include("partials/footer"); %>
